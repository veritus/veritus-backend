[MASTER]
ignore=apps.py, admin.py, migrations, bin, lib, man
load-plugins=pylint_django

[MESSAGES CONTROL]
disable=missing-docstring, broad-except, no-self-use, invalid-name

[TYPECHECK]
ignored-classes=WSGIRequest, Exception, Client


[pre-commit-hook]
command=custom_pylint
params=--rcfile=/path/to/another/pylint.rc
limit=8.0